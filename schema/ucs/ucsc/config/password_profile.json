{
   "id": "ucs_central_config_password_profile#",
   "$schema": "http://json-schema.org/draft-07/schema#",
   "title": "EasyUCS ucs central config password_profile section",
   "description": "EasyUCS ucs central config password_profile section JSON schema",
   "type": "object",
   "properties": {
      "comment": {
         "description": "Password Profile Comments",
         "type": "string",
         "default": ""
      },
      "password_strength_check": {
         "description": "Password Profile Password Strength Check",
         "type": "string",
         "default": "yes",
         "enum": [
            "yes",
            "no"
         ]
      },
      "change_interval": {
         "description": "Password Profile Password Change Interval",
         "type": "string",
         "default": "48"
      },
      "no_change_interval": {
         "description": "Password Profile Password No Change Interval",
         "type": "string",
         "default": "24"
      },
      "change_during_interval": {
         "description": "Password Profile Password Change During Interval",
         "type": "string",
         "default": "enable",
         "enum": [
            "enable",
            "disable"
         ]
      },
      "history_count": {
         "description": "Password Profile Password History Count",
         "type": "string",
         "default": "5"
      },
      "change_count": {
         "description": "Password Profile Password Change Count",
         "type": "string",
         "default": "2"
      }
   },
   "additionalProperties": false
}